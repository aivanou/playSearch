# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
POST          /search              controllers.SearchController.search()

GET           /version             controllers.VersionController.getVersion()

#ok-catalog

POST          /catalog             controllers.CatalogController.insertCatalog()
GET           /catalog/:url        controllers.CatalogController.getCatalog(url:String)
DELETE        /catalog/:url        controllers.CatalogController.deleteCatalog(url:String)

# Map static resources from the /public folder to the /assets URL path
GET           /assets/*file        controllers.Assets.at(path="/public", file)
